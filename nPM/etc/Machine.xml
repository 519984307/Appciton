    <ECG SptProvider="DemoProvider,BLM_E5">BLM_E5</ECG>
    <RESP SptProvider="DemoProvider,BLM_E5">BLM_E5</RESP>
    <SPO2 SptProvider="DemoProvider,BLM_SPO2,BLM_TS3,BLM_S5,MASIMO_SPO2,NELLCOR_SPO2">NELLCOR_SPO2</SPO2>
