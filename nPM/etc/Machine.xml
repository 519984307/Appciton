<?xml version="1.0" encoding="UTF-8"?>
<Machine>
    <MachineName>nPM</MachineName>
    <!--屏幕分辨率-->
    <ScreenWidth>1280</ScreenWidth>
    <ScreenHeight>800</ScreenHeight>
    <ScreenPixelWPitch>0.1695</ScreenPixelWPitch>
    <ScreenPixelHPitch>0.1695</ScreenPixelHPitch>
    <!--功能机型的使能配置-->
    <ConfiguredFunctions>ECG12LEADS,SPO2,NIBP,CO2,IBP,AG,CO,TEMP,WIFI</ConfiguredFunctions>
    <ECG12LEADEnable>1</ECG12LEADEnable>
    <SPO2Enable>1</SPO2Enable>
    <NIBPEnable>1</NIBPEnable>
    <RESPEnable>1</RESPEnable>
    <CO2Enable>1</CO2Enable>
    <AGEnable>1</AGEnable>
