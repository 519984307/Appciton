    <IBPEnable>1</IBPEnable>
    <TEMPEnable>1</TEMPEnable>
    <WIFIEnable>1</WIFIEnable>
    <!--系统信息-->
    <SerialNumber>20180725222</SerialNumber>
    <SoftwareVersion>0.0.0.1</SoftwareVersion>
    <!--参数信息-->
    <AllParams>ECG,SPO2,RESP,NIBP,CO2,IBP,AG,CO,TEMP,PD</AllParams>
