<?xml version="1.0" encoding="UTF-8"?>
<Machine>
    <MachineName>nPM</MachineName>
    <!--功能机型的使能配置-->
    <ConfiguredFunctions>ECG12LEAD,SPO2,NIBP,RESP,CO2,AG,CO,IBP,TEMP,Printer,WIFI,Touch,O2,NIBPNEOMeasure,HDMI,NurseCall,AnalogOutput,SyncDefibrillation</ConfiguredFunctions>
    <ECG12LEADEnable>1</ECG12LEADEnable>
    <SPO2Enable>1</SPO2Enable>
    <NIBPEnable>1</NIBPEnable>
    <RESPEnable>1</RESPEnable>
    <CO2Enable>1</CO2Enable>
    <AGEnable>0</AGEnable>
    <COEnable>0</COEnable>
    <IBPEnable>0</IBPEnable>
    <TEMPEnable>1</TEMPEnable>
    <PrinterEnable>1</PrinterEnable>
    <WIFIEnable>0</WIFIEnable>
    <TouchEnable>0</TouchEnable>
    <NurseCallEnable>1</NurseCallEnable>
    <O2Enable>1</O2Enable>
    <ScreenInfoEnable>2</ScreenInfoEnable>
    <!--屏幕信息-->
    <ScreenInfo>
	<size8Inch>
	    <ScreenPixelWPitch>0.158</ScreenPixelWPitch>
    	    <ScreenPixelHPitch>0.158</ScreenPixelHPitch>
    	    <ScreenSize>8.0</ScreenSize>
        </size8Inch>
	<size10Inch>
	    <ScreenPixelWPitch>0.1695</ScreenPixelWPitch>
    	    <ScreenPixelHPitch>0.1695</ScreenPixelHPitch>
    	    <ScreenSize>10.1</ScreenSize>
	</size10Inch>
	<size12Inch>
	    <ScreenPixelWPitch>0.204</ScreenPixelWPitch>
    	    <ScreenPixelHPitch>0.204</ScreenPixelHPitch>
    	    <ScreenSize>12.1</ScreenSize>
	</size12Inch>
    </ScreenInfo>
    <!--屏幕类型选择  0:商业屏,1:工业屏-->
    <ScreenTypeSelect>0</ScreenTypeSelect>
    <NIBPNEOMeasureEnable>1</NIBPNEOMeasureEnable>	
    <HDMIEnable>0</HDMIEnable>
    <AnalogOutputEnable>0</AnalogOutputEnable>
    <SyncDefibrillationEnable>0</SyncDefibrillationEnable>	
    <!--背光调节方式  0:默认为等级亮度递增方式调节,1:如果0对应模式无法达到亮度递增调节效果，就选择该模式1-->
    <!--总之，2个模式中要选择能使得背光等级亮度递增的调节模式-->
    <BacklightAdjustment>0</BacklightAdjustment>
    <!--机器是否为新生儿专用机器-->
    <NeonateMachine>0</NeonateMachine>
    <!--系统信息-->
    <SerialNumber>20180725222</SerialNumber>
    <SoftwareVersion>0.0.0.1</SoftwareVersion>
    <!--参数信息-->
    <AllParams>ECG,SPO2,RESP,NIBP,CO2,IBP,AG,CO,TEMP,O2</AllParams>
    <!--数据记录使能选择-->
    <Record>
        <ECG>0</ECG>
        <SPO2>0</SPO2>
        <Battery>0</Battery>
        <NIBP>0</NIBP>
	<CO2>0</CO2>
        <TEMP>0</TEMP>
    </Record>
    <!--参数与Provider的关系-->
    <ECG SptProvider="DemoProvider,BLM_E5">BLM_E5</ECG>
    <RESP SptProvider="DemoProvider,BLM_E5">BLM_E5</RESP>
    <SPO2 SptProvider="DemoProvider,BLM_S5,MASIMO_SPO2,RAINBOW_SPO2">RAINBOW_SPO2</SPO2>
    <NIBP SptProvider="DemoProvider,BLM_N5,SUNTECH_NIBP">BLM_N5</NIBP>
    <CO2 SptProvider="DemoProvider,MASIMO_CO2">MASIMO_CO2</CO2>
    <IBP SptProvider="DemoProvider,WITLEAF_IBP">WITLEAF_IBP</IBP>
    <AG SptProvider="DemoProvider,PHASEIN_AG">PHASEIN_AG</AG>
    <CO SptProvider="DemoProvider,WITLEAF_IBP">WITLEAF_IBP</CO>
    <TEMP SptProvider="DemoProvider,BLM_TT3,BLM_T5">BLM_T5</TEMP>
    <O2 SptProvider="DemoProvider,NEONATE_O2">NEONATE_O2</O2>
    <!--Provider端口信息-->
    <PRT48Port>/dev/ttyS3</PRT48Port>
    <BLM_E5Port>/dev/ttyS1</BLM_E5Port>
    <BLM_N5Port>/dev/ttyPM1</BLM_N5Port>
    <SUNTECH_NIBPPort>/dev/ttyPM1</SUNTECH_NIBPPort>
    <BLM_S5Port>DataDispatcher</BLM_S5Port>
    <MASIMO_SPO2Port>DataDispatcher</MASIMO_SPO2Port>
    <RAINBOW_SPO2Port>DataDispatcher</RAINBOW_SPO2Port>
    <RAINBOW_SPO2PlugInPort>PlugIn</RAINBOW_SPO2PlugInPort>
    <SystemBoardPort>/dev/ttyPM0</SystemBoardPort>
    <BLM_T5Port>DataDispatcher</BLM_T5Port>
    <WITLEAF_IBPPort>/dev/ttyS4</WITLEAF_IBPPort>
    <MASIMO_CO2Port>/dev/ttyPM2</MASIMO_CO2Port>
    <DataDispatcherPort>/dev/ttyPM3</DataDispatcherPort>
    <NEONATE_O2Port>/dev/ttyS5</NEONATE_O2Port>
    <PlugInPort>/dev/ttyS2</PlugInPort>
    <!--    <PHASEIN_AGPort>/dev/ttyWK3</PHASEIN_AGPort>-->
</Machine>
